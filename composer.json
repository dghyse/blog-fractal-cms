{
  "name": "dghyse/blog-fractal-cms",
  "description": "Projet squelette Blog basé sur FractalCMS – prêt à l'emploi pour démarrer rapidement.",
  "keywords": ["blog", "blog", "fractal-cms", "fractalCms"],
  "homepage": "https://github.com/dghyse/blog-fractal-cms",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/dghyse/blog-fractal-cms/issues",
    "source": "https://github.com/dghyse/blog-fractal-cms"
  },
  "authors": [
    {
      "name": "David Ghyse",
      "email": "davidg@webcraftdg.fr"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=8.2",
    "dghyse/fractal-cms": "~v1.5"
  },
  "require-dev" : {
    "yiisoft/yii2-debug":"~2.0",
    "yiisoft/yii2-gii":"~2.0"
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "autoload": {
    "psr-4": {
      "console\\": "console",
      "app\\": "common",
      "webapp\\": "webapp"
    }
  },
  "scripts": {
    "post-install-cmd": [
    ]
  }
}
